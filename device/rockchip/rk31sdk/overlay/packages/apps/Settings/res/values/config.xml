<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="additional_system_update">android.rockchip.update.service</string>
    <string name="additional_system_update_menu">android.rockchip.update.service.Setting</string>

    <!-- Settings:Accessibility Setting sub preference item config -->
    <add-resource type="bool" name="config_enableServiceCategoryPreference" />
    <bool name="config_enableServiceCategoryPreference">true</bool>

    <add-resource type="bool" name="config_enableSystemsCategoryPreference" />
    <bool name="config_enableSystemsCategoryPreference">true</bool>
    <!-- accessibility: systems category preference sub item, config_enableSystemsCategoryPreference must be true-->

    <add-resource type="bool" name="config_enableScreenMagnificationPreference" />
    <bool name="config_enableScreenMagnificationPreference">false</bool>

    <add-resource type="bool" name="config_enableLargeTextPreference" />
    <bool name="config_enableLargeTextPreference">true</bool>

    <add-resource type="bool" name="config_enableRotateScreenPreference" />
    <bool name="config_enableRotateScreenPreference">true</bool>

    <add-resource type="bool" name="config_enableSpeakPasswordPreference" />
    <bool name="config_enableSpeakPasswordPreference">false</bool>

    <add-resource type="bool" name="config_enableGlobalGesturePreference" />
    <bool name="config_enableGlobalGesturePreference">false</bool>

    <add-resource type="bool" name="config_enableTtsSettingsPreference" />
    <bool name="config_enableTtsSettingsPreference">true</bool>

    <add-resource type="bool" name="config_enablePressTimeoutPreference" />
    <bool name="config_enablePressTimeoutPreference">false</bool>

    <add-resource type="bool" name="config_enableScriptInjectionPreference" />
    <bool name="config_enableScriptInjectionPreference">true</bool>
    <!--end system category-->
    <!--end Accessibility Setting-->

</resources>
